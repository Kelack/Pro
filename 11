//11. Напечатать   те   элементы   последовательности   натуральных   чисел n0 ,n1,...,nm , которые делятся на сумму своих цифр.

$n = 100;

for($i = 1; $i !== $n+1; $i++)
{
    $number = $i;
    $sum = 0;
    while($number > 0)
    {
        $sum += $number % 10;
       
        $number = intdiv($number,10);
    }
    
    if($i % $sum == 0)
    {
        echo $i .' ';
    }
    
    
    
}
